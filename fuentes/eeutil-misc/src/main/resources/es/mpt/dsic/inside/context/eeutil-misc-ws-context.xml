	<jaxws:endpoint id="eeUtilServiceWS" implementor="#eeUtilService"
		address="/EeUtilService">
		<jaxws:features>
				<!-- esto genera logging de las peticiones de ws -->			
			<!--  <bean class="org.apache.cxf.feature.LoggingFeature" /> -->
			<bean
				class="es.mpt.dsic.inside.security.implementacion.SpringSecurityContextFeature" />
		</jaxws:features>
		<jaxws:properties>
			<entry key="schema-validation-enabled" value="true" />
			<entry key="ws-security.callback-handler">
				<bean
					class="es.mpt.dsic.inside.security.implementacion.ServerPasswordCallbackHandler">
					<property name="logExceptions" value="false" />
					<property name="nestExceptions" value="true" />
					<property name="authenticationManager" ref="autenticationManager" />
				</bean>
			</entry>
		</jaxws:properties>
		<jaxws:inInterceptors>
<!-- 			<ref bean="customLoggingInInterceptor"/> -->
			<bean
				class="es.mpt.dsic.inside.security.interceptor.SoapAuthenticationObjectInterceptor" >
				<property name="authenticationManager" ref="autenticationManager" />
				<property name="authenticationRequired" value="true" />
			</bean>
			<bean
				class="es.mpt.dsic.inside.security.interceptor.SoapAuthenticationInterceptor">
				<property name="authenticationManager" ref="autenticationManager" />
				<property name="authenticationRequired" value="false" />
			</bean>
		</jaxws:inInterceptors>
<!-- 		<jaxws:outInterceptors> -->
<!-- 			<ref bean="loggingOutInterceptor"/> -->
<!-- 		</jaxws:outInterceptors> -->
	</jaxws:endpoint>
	
